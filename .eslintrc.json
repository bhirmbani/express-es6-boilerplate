{
  "extends": ["plugin:flowtype/recommended", "airbnb-base"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "plugins": ["flowtype"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "node": true
  }
}
